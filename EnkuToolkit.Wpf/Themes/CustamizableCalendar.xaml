<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:controls="clr-namespace:EnkuToolkit.Wpf.Controls"
                    xmlns:internal="clr-namespace:EnkuToolkit.Wpf.Controls.Internals.CustamizableCalendar"
                    xmlns:datas="clr-namespace:EnkuToolkit.UiIndependent.CustamizableCalendarDatas;assembly=EnkuToolkit.UiIndependent"
                    xmlns:me="clr-namespace:EnkuToolkit.Wpf.MarkupExtensions"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style TargetType="controls:CustamizableCalendar">
        <!--#region プロパティの初期値指定用セッター群-->
        <Setter Property="DayOfWeekTemplate">
            <Setter.Value>
                <DataTemplate DataType="datas:DayOfWeekSource" >
                    <Label Content="{Binding Text}" 
                           HorizontalAlignment="Center" 
                           VerticalAlignment="Center">

                        <Label.Style>
                            <Style TargetType="Label" BasedOn="{me:GetApplicationResource {x:Type Label}}">
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding IsHoliday}" Value="True">
                                        <Setter Property="Foreground" Value="Red" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Label.Style>
                    </Label>
                </DataTemplate>
            </Setter.Value>
        </Setter>

        <Setter Property="CellTemplate">
            <Setter.Value>
                <DataTemplate DataType="datas:CalendarSource">
                    <Border CornerRadius="10" 
                            HorizontalAlignment="Center" 
                            VerticalAlignment="Center">
                        <Label Content="{Binding Date.Day}" />

                        <Border.Style>
                            <Style TargetType="Border">
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding IsToday}" Value="True">
                                        <Setter Property="Background" Value="ForestGreen" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Border.Style>
                    </Border>
                </DataTemplate>
            </Setter.Value>
        </Setter>

        <Setter Property="AutoGenCellTemplate">
            <Setter.Value>
                <DataTemplate DataType="datas:CalendarSource">
                    <Border CornerRadius="10" 
                            HorizontalAlignment="Center" 
                            VerticalAlignment="Center">
                        <Label Content="{Binding Date.Day}" />

                        <Border.Style>
                            <Style TargetType="Border">
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding IsToday}" Value="True">
                                        <Setter Property="Background" Value="ForestGreen" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Border.Style>
                    </Border>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <!--#endregion-->

        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="controls:CustamizableCalendar">
                    <DockPanel LastChildFill="True">
                        <!--#region ヘッダー-->
                        <UniformGrid Columns="3"
                                     DockPanel.Dock="Top"
                                     HorizontalAlignment="Center">
                            <Button Margin="5" 
                                    Background="Transparent"
                                    BorderBrush="Transparent"
                                    Command="{TemplateBinding ToLastMonthButtonCommand}"
                                    HorizontalAlignment="Left">
                                <Polygon Points="14,0 14,14, 0,7" 
                                         Stroke="{Binding ElementName=YearAndMonthLabel, Path=Foreground}" 
                                         Fill="{Binding ElementName=YearAndMonthLabel, Path=Foreground}"  />
                            </Button>

                            <Label Name="YearAndMonthLabel" 
                                   Content="{TemplateBinding TargetYearAndMonth}" 
                                   FontSize="15" Padding="0" 
                                   VerticalAlignment="Center"
                                   HorizontalAlignment="Center"/>

                            <Button Margin="5" 
                                    Background="Transparent"
                                    BorderBrush="Transparent"
                                    Command="{TemplateBinding ToNextMonthButtonCommand}"
                                    HorizontalAlignment="Right">
                                <Polygon Points="0,0 0,14, 14,7"
                                         Stroke="{Binding ElementName=YearAndMonthLabel, Path=Foreground}" 
                                         Fill="{Binding ElementName=YearAndMonthLabel, Path=Foreground}"  />
                            </Button>

                            <UniformGrid.Style>
                                <Style TargetType="UniformGrid">
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=IsShowHeader}" Value="True">
                                            <Setter Property="Visibility" Value="Visible" />
                                        </DataTrigger>

                                        <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=IsShowHeader}" Value="False">
                                            <Setter Property="Visibility" Value="Collapsed" />
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </UniformGrid.Style>
                        </UniformGrid>
                        <!--#endregion-->

                        <ScrollViewer DockPanel.Dock="Bottom"
                                      VerticalScrollBarVisibility="Auto"
                                      HorizontalScrollBarVisibility="Auto">
                            <StackPanel>
                                <!--region 曜日表示行-->
                                <StackPanel Name="DayOfWeekRows" 
                                            HorizontalAlignment="Center"
                                            Orientation="Horizontal">
                                    <StackPanel.DataContext>
                                        <MultiBinding Converter="{x:Static internal:DayOfWeekSourceToItemsSourceConverter.Instance}">
                                            <Binding RelativeSource="{RelativeSource Mode=TemplatedParent}" Path="DayOfWeekSource"/>
                                            <Binding RelativeSource="{RelativeSource Mode=TemplatedParent}" Path="IsStartWeekMonday" />
                                        </MultiBinding>
                                    </StackPanel.DataContext>

                                    <ContentControl Content="{Binding ElementName=DayOfWeekRows, Path=DataContext[0]}"
                                            Width="{Binding ElementName=CelendarCel0Row0Col, Path=ActualWidth}"
                                            ContentTemplate="{TemplateBinding DayOfWeekTemplate}" />

                                    <ContentControl Content="{Binding ElementName=DayOfWeekRows, Path=DataContext[1]}"
                                            Width="{Binding ElementName=CelendarCel0Row1Col, Path=ActualWidth}"
                                            ContentTemplate="{TemplateBinding DayOfWeekTemplate}" />

                                    <ContentControl Content="{Binding ElementName=DayOfWeekRows, Path=DataContext[2]}"
                                            Width="{Binding ElementName=CelendarCel0Row2Col, Path=ActualWidth}"
                                            ContentTemplate="{TemplateBinding DayOfWeekTemplate}" />

                                    <ContentControl Content="{Binding ElementName=DayOfWeekRows, Path=DataContext[3]}"
                                            Width="{Binding ElementName=CelendarCel0Row3Col, Path=ActualWidth}"
                                            ContentTemplate="{TemplateBinding DayOfWeekTemplate}" />

                                    <ContentControl Content="{Binding ElementName=DayOfWeekRows, Path=DataContext[4]}"
                                            Width="{Binding ElementName=CelendarCel0Row4Col, Path=ActualWidth}"
                                            ContentTemplate="{TemplateBinding DayOfWeekTemplate}" />

                                    <ContentControl Content="{Binding ElementName=DayOfWeekRows, Path=DataContext[5]}"
                                            Width="{Binding ElementName=CelendarCel0Row5Col, Path=ActualWidth}"
                                            ContentTemplate="{TemplateBinding DayOfWeekTemplate}" />

                                    <ContentControl Content="{Binding ElementName=DayOfWeekRows, Path=DataContext[6]}"
                                            Width="{Binding ElementName=CelendarCel0Row6Col, Path=ActualWidth}"
                                            ContentTemplate="{TemplateBinding DayOfWeekTemplate}" />
                                </StackPanel>
                                <!--#endregion-->

                                <!--#region カレンダーセル-->
                                <ListBox DockPanel.Dock="Bottom"
                                         HorizontalAlignment="Center"
                                         SelectionMode="{TemplateBinding SelectionMode}"
                                         internal:ListboxUnselectAllOnDataContextChangedBehavior.IsApply="True"
                                         Name="listBox"
                                         ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                                         ScrollViewer.VerticalScrollBarVisibility="Disabled">
                                    <ListBox.DataContext>
                                        <MultiBinding Converter="{x:Static internal:SourceToItemsConverter.Instance}">
                                            <Binding RelativeSource="{RelativeSource Mode=TemplatedParent}" Path="Source"/>
                                            <Binding RelativeSource="{RelativeSource Mode=TemplatedParent}" Path="IsStartWeekMonday" />
                                            <Binding RelativeSource="{RelativeSource Mode=TemplatedParent}" Path="CellTemplate" />
                                            <Binding RelativeSource="{RelativeSource Mode=TemplatedParent}" Path="AutoGenCellTemplate" />
                                            <Binding RelativeSource="{RelativeSource Mode=TemplatedParent}" Path="TargetYearAndMonth" />
                                        </MultiBinding>
                                    </ListBox.DataContext>

                                    <ListBox.ItemsPanel>
                                        <ItemsPanelTemplate>
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="Auto" />
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto" />
                                                    <RowDefinition Height="Auto" />
                                                    <RowDefinition Height="Auto" />
                                                    <RowDefinition Height="Auto" />
                                                    <RowDefinition Height="Auto" />
                                                    <RowDefinition Height="Auto" />
                                                </Grid.RowDefinitions>
                                            </Grid>
                                        </ItemsPanelTemplate>
                                    </ListBox.ItemsPanel>

                                    <ListBox.ItemContainerStyle>
                                        <Style TargetType="ListBoxItem" BasedOn="{me:GetApplicationResourceExtension {x:Type ListBoxItem}}">
                                            <Setter Property="HorizontalContentAlignment" Value="Stretch" />
                                            <Setter Property="VerticalContentAlignment" Value="Stretch" />
                                            <Setter Property="BorderThickness" Value="1"/>
                                            <Setter Property="Margin" Value="0"/>
                                            <Setter Property="Padding" Value="0"/>
                                        </Style>
                                    </ListBox.ItemContainerStyle>

                                    <ListBox.Items>
                                        <ListBoxItem Grid.Column="0" Grid.Row="0" Name="CelendarCel0Row0Col"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[0].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[0].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[0].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="1" Grid.Row="0" Name="CelendarCel0Row1Col"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[1].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[1].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[1].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="2" Grid.Row="0" Name="CelendarCel0Row2Col"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[2].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[2].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[2].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="3" Grid.Row="0"  Name="CelendarCel0Row3Col"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[3].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[3].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[3].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="4" Grid.Row="0"  Name="CelendarCel0Row4Col"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[4].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[4].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[4].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="5" Grid.Row="0" Name="CelendarCel0Row5Col"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[5].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[5].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[5].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="6" Grid.Row="0" Name="CelendarCel0Row6Col"
