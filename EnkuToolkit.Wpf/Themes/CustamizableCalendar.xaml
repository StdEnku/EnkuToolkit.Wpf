                                                     Content="{Binding ElementName=listBox, Path=DataContext[10].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[10].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="4" Grid.Row="1"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[11].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[11].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[11].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="5" Grid.Row="1"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[12].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[12].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[12].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="6" Grid.Row="1"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[13].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[13].Source}"
