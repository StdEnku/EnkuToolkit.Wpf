                                                     Content="{Binding ElementName=listBox, Path=DataContext[17].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[17].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="4" Grid.Row="2"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[18].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[18].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[18].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="5" Grid.Row="2"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[19].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[19].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[19].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="6" Grid.Row="2"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[20].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[20].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[20].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="0" Grid.Row="3"
