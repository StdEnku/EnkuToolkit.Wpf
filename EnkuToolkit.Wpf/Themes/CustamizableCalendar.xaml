                                                     Content="{Binding ElementName=listBox, Path=DataContext[21].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[21].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="1" Grid.Row="3"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[22].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[22].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[22].DataTemplate}"/>

