                                        <ListBoxItem Grid.Column="6" Grid.Row="5"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[41].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[41].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[41].DataTemplate}"/>
