                                                     Content="{Binding ElementName=listBox, Path=DataContext[23].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[23].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="3" Grid.Row="3"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[24].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[24].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[24].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="4" Grid.Row="3"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[25].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[25].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[25].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="5" Grid.Row="3"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[26].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[26].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[26].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="6" Grid.Row="3"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[27].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[27].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[27].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="0" Grid.Row="4"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[28].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[28].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[28].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="1" Grid.Row="4"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[29].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[29].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[29].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="2" Grid.Row="4"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[30].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[30].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[30].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="3" Grid.Row="4"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[31].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[31].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[31].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="4" Grid.Row="4"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[32].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[32].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[32].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="5" Grid.Row="4"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[33].IsEnabled}"
