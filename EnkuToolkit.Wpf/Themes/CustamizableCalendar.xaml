                                        <ListBoxItem Grid.Column="0" Grid.Row="2"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[14].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[14].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[14].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="1" Grid.Row="2"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[15].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[15].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[15].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="2" Grid.Row="2"
                                                     IsEnabled="{Binding ElementName=listBox, Path=DataContext[16].IsEnabled}"
                                                     Content="{Binding ElementName=listBox, Path=DataContext[16].Source}"
                                                     ContentTemplate="{Binding ElementName=listBox,Path=DataContext[16].DataTemplate}"/>

                                        <ListBoxItem Grid.Column="3" Grid.Row="2"
