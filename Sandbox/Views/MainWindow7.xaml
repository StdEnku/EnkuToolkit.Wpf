<et:CustomizableTitlebarAnimatedNavigationWindow
        x:Class="Sandbox.Views.MainWindow7"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sandbox.Views"
        xmlns:et="https://github.com/StdEnku/EnkuToolkit"
        Source="Page1.xaml"
        TransitionEffect="HorizontalModernSlide"
        mc:Ignorable="d"
        et:WindowExtensionBehavior.StateSavePath="./Hoge.json"
        et:NavigationWindowExtensionBehavior.IsSendNavigationParam="True"
        CornerRadius="20"
        Title="MainWindow7" Height="450" Width="800">

    <et:CustomizableTitlebarAnimatedNavigationWindow.Titlebar>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
            <Button Content="_" WindowChrome.IsHitTestVisibleInChrome="True" Click="Button_Click" />
            <Button Content="□" WindowChrome.IsHitTestVisibleInChrome="True" Click="Button_Click_1" />
            <Button Content="×" WindowChrome.IsHitTestVisibleInChrome="True" Click="Button_Click_2" />
        </StackPanel>
    </et:CustomizableTitlebarAnimatedNavigationWindow.Titlebar>
</et:CustomizableTitlebarAnimatedNavigationWindow>
